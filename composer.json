{
    "name": "fullstack/inbounder",
    "description": "A Laravel package for handling Mailgun inbound emails with attachments and event-driven processing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fullstack Team",
            "email": "team@fullstack.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fullstack\\Inbounder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fullstack\\Inbounder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fullstack\\Inbounder\\InbounderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
